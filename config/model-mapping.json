  "routingStrategies": {
    "cost_optimized": {
      "description": "Route to lowest cost model that meets requirements",
      "priorityFactors": ["costPerToken", "responseTime", "availability"]
    },
    "performance_optimized": {
      "description": "Route to fastest model that meets requirements",
      "priorityFactors": ["responseTime", "throughput", "availability"]
    },
    "feature_optimized": {
      "description": "Route to model with best feature support",
      "priorityFactors": ["supportedFeatures", "accuracy", "availability"]
    },
    "balanced": {
      "description": "Balance cost, performance, and features",
      "priorityFactors": ["availability", "costPerToken", "responseTime", "supportedFeatures"]
    }
  },
