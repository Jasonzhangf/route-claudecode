{
  // RCC v4.0 Provider配置示例
  "version": "4.0.0-alpha.1",
  
  // 全局配置
  "global": {
    "debug": true,
    "logLevel": "info"
  },
  
  // Provider配置列表
  "providers": [
    // OpenAI官方API
    {
      "id": "openai-official",
      "name": "OpenAI Official API",
      "type": "openai",
      "enabled": true,
      "config": {
        "apiKey": "sk-your-openai-api-key-here",
        "defaultModel": "gpt-4o",
        "timeout": 30000,
        "maxRetries": 3,
        "enableStreaming": true,
        "enableToolCalls": true,
        "debug": false
      }
    },
    
    // Anthropic官方API
    {
      "id": "anthropic-official", 
      "name": "Anthropic Official API",
      "type": "anthropic",
      "enabled": true,
      "config": {
        "apiKey": "your-anthropic-api-key-here",
        "defaultModel": "claude-3-5-sonnet-20241022",
        "timeout": 30000,
        "maxRetries": 3,
        "enableToolCalls": true,
        "debug": false
      }
    },
    
    // OpenAI兼容服务 - LM Studio本地
    {
      "id": "lmstudio-local",
      "name": "LM Studio Local",
      "type": "openai",
      "enabled": false, // 默认禁用，需要本地LM Studio运行
      "config": {
        "apiKey": "not-required",
        "baseURL": "http://localhost:1234/v1",
        "defaultModel": "qwen2.5-coder-32b",
        "timeout": 60000,
        "maxRetries": 2,
        "enableStreaming": false,
        "enableToolCalls": true,
        "debug": true
      }
    },
    
    // OpenAI兼容服务 - 自定义端点
    {
      "id": "custom-openai-compatible",
      "name": "Custom OpenAI Compatible Service",
      "type": "openai", 
      "enabled": false, // 默认禁用，需要配置实际端点
      "config": {
        "apiKey": "your-custom-api-key",
        "baseURL": "https://your-custom-endpoint.com/v1",
        "defaultModel": "custom-model-name",
        "timeout": 45000,
        "maxRetries": 3,
        "enableStreaming": true,
        "enableToolCalls": true,
        "debug": false
      }
    }
  ]
}