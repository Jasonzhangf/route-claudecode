/**
 * CodeWhisperer 实时流式配置示例
 * 用于测试实时流式实现
 */
{
  "implementation": "realtime",
  "realtimeOptions": {
    "enableZeroDelay": true,
    "maxConcurrentStreams": 50,
    "binaryFrameSize": 1048576,
    "toolCallStrategy": "immediate",
    "enableCompression": false
  },
  "performanceMetrics": {
    "enableProfiling": true,
    "collectLatencyData": true,
    "memoryUsageTracking": true,
    "metricsIntervalMs": 2000
  },
  "fallback": {
    "enableFallback": true,
    "fallbackToBuffered": true,
    "maxFailuresBeforeFallback": 3
  }
}