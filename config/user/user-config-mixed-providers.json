{
  "server": {
    "port": 3456,
    "host": "localhost"
  },
  "providers": {
    "lmstudio": {
      "endpoint": "http://localhost:1234/v1/chat/completions",
      "models": ["qwen3-30b", "glm-4.5-air"]
    },
    "shuaihong": {
      "endpoint": "https://ai.shuaihong.fun/v1/chat/completions",
      "apiKey": "${SHUAIHONG_API_KEY}",
      "models": ["claude-4-sonnet", "gemini-2.5-pro", "o1-preview"]
    },
    "modelscope": {
      "endpoint": "https://dashscope.aliyuncs.com/v1/chat/completions",
      "apiKey": "${MODELSCOPE_API_KEY}",
      "models": ["qwen-turbo", "qwen-plus", "qwen-max"]
    },
    "google": {
      "endpoint": "https://generativelanguage.googleapis.com/v1/models/gemini-2.5-pro:generateContent",
      "apiKey": "${GOOGLE_GEMINI_API_KEY}",
      "models": ["gemini-2.5-pro", "gemini-2.5-flash"]
    }
  },
  "routing": {
    "default": {
      "provider": "lmstudio",
      "model": "qwen3-30b"
    },
    "background": {
      "provider": "modelscope",
      "model": "qwen-turbo"
    },
    "thinking": {
      "provider": "shuaihong",
      "model": "claude-4-sonnet"
    },
    "longcontext": {
      "provider": "google",
      "model": "gemini-2.5-pro"
    },
    "search": {
      "provider": "shuaihong",
      "model": "o1-preview"
    }
  },
  "debug": {
    "enabled": true,
    "logLevel": "info"
  }
}