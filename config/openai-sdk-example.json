{
  "server": {
    "port": 5509,
    "host": "localhost"
  },
  "providers": {
    "openai-official": {
      "type": "openai",
      "endpoint": "https://api.openai.com/v1",
      "clientType": "sdk",
      "authentication": {
        "type": "bearer",
        "credentials": {
          "apiKey": "your-openai-api-key"
        }
      },
      "models": [
        "gpt-4o",
        "gpt-4o-mini",
        "gpt-4-turbo"
      ],
      "defaultModel": "gpt-4o-mini",
      "maxTokens": {
        "gpt-4o": 128000,
        "gpt-4o-mini": 128000,
        "gpt-4-turbo": 128000
      },
      "sdkOptions": {
        "timeout": 60000,
        "maxRetries": 3,
        "defaultHeaders": {
          "X-Custom-Header": "claude-code-router"
        }
      },
      "description": "Official OpenAI API using SDK client"
    },
    "openai-compatible": {
      "type": "openai",
      "endpoint": "https://api-inference.modelscope.cn/v1",
      "clientType": "enhanced",
      "authentication": {
        "type": "bearer",
        "credentials": {
          "apiKey": "your-modelscope-key"
        }
      },
      "models": [
        "Qwen/Qwen3-Coder-480B-A35B-Instruct"
      ],
      "defaultModel": "Qwen/Qwen3-Coder-480B-A35B-Instruct",
      "maxTokens": {
        "Qwen/Qwen3-Coder-480B-A35B-Instruct": 262144
      },
      "description": "ModelScope API using Enhanced client"
    },
    "openai-auto-switch": {
      "type": "openai",
      "endpoint": "https://api.openai.com/v1",
      "clientType": "auto",
      "authentication": {
        "type": "bearer",
        "credentials": {
          "apiKey": "your-openai-api-key"
        }
      },
      "models": [
        "gpt-4o-mini"
      ],
      "defaultModel": "gpt-4o-mini",
      "switchingStrategy": {
        "autoSwitch": true,
        "errorThreshold": 3,
        "performanceThreshold": 10000,
        "switchCooldown": 60000
      },
      "sdkOptions": {
        "timeout": 30000,
        "maxRetries": 2
      },
      "description": "Auto-switching between Enhanced and SDK clients"
    }
  },
  "routing": {
    "default": {
      "provider": "openai-auto-switch",
      "model": "gpt-4o-mini"
    },
    "background": {
      "provider": "openai-official",
      "model": "gpt-4o-mini"
    },
    "thinking": {
      "provider": "openai-official",
      "model": "gpt-4o"
    },
    "longcontext": {
      "provider": "openai-compatible",
      "model": "Qwen/Qwen3-Coder-480B-A35B-Instruct"
    },
    "search": {
      "provider": "openai-official",
      "model": "gpt-4o-mini"
    }
  },
  "debug": {
    "enabled": true,
    "logLevel": "debug",
    "traceRequests": true,
    "saveRequests": true,
    "logDir": "/tmp/openai-sdk-logs"
  },
  "hooks": []
}