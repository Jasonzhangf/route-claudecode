#!/usr/bin/env node

/**
 * rcc3 - Claude Code Router V3.0 CLI Executable
 * Six-layer architecture with SDK integration
 */

import { fileURLToPath } from 'url';
import path from 'path';

// Get current directory in ES module
const __filename = fileURLToPath(import.meta.url);
const __dirname = path.dirname(__filename);

// Import the compiled V3 CLI module
const cliPath = path.join(__dirname, 'dist', 'v3-cli.js');
import(cliPath).catch(error => {
  console.error('‚ùå rcc3: Failed to load CLI module:', error.message);
  process.exit(1);
});