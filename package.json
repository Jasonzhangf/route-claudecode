{
  "name": "route-claude-code",
  "version": "4.0.0-alpha.1",
  "description": "高性能多AI提供商路由转换系统 - v4.0重构版",
  "bin": {
    "rcc4": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && shx cp -r config dist/",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "shx rm -rf dist coverage",
    "prepare": "npm run build",
    "docs": "typedoc src --out docs/api",
    "debug": "node --inspect dist/cli.js"
  },
  "keywords": [
    "claude",
    "code",
    "router",
    "llm",
    "anthropic",
    "openai",
    "lmstudio",
    "ai-proxy",
    "typescript"
  ],
  "author": "Jason Zhang",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google-ai/generativelanguage": "^3.3.0",
    "commander": "^14.0.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.4.0",
    "json5": "^2.2.3",
    "openai": "^5.12.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.15",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "shx": "^0.4.0",
    "ts-jest": "^29.2.4",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanzhang16/route-claudecode.git"
  },
  "bugs": {
    "url": "https://github.com/fanzhang16/route-claudecode/issues"
  },
  "homepage": "https://github.com/fanzhang16/route-claudecode#readme"
}
