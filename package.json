{
  "name": "route-claude-code",
  "version": "4.1.0",
  "description": "Route Claude Code v4.1 - Enhanced OpenAI & Qwen Support with Mixed Configuration",
  "main": "dist/cli.js",
  "bin": {
    "rcc4": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "type-check": "tsc --noEmit",
    "type-check-strict": "tsc --noEmit --strict",
    "start": "node dist/cli.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest --coverage --json --outputFile=./test-results/unit-test-report.json",
    "test:unit": "jest tests/unit/",
    "test:integration": "jest tests/integration/",
    "test:watch": "jest --watch",
    "test:pipeline": "ts-node src/api/test-pipeline-integration.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "tsc --noEmit",
    "lint:fix": "prettier --write \"src/**/*.{ts,js,json,md}\" && tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "babel-jest": "^30.0.5",
    "jest": "^30.0.5",
    "jest-json-reporter": "^1.2.2",
    "json5": "^2.2.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@google-ai/generativelanguage": "^2.7.0",
    "@types/commander": "^2.12.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "open": "^10.2.0",
    "openai": "^4.67.3",
    "tiktoken": "^1.0.22"
  },
  "files": [
    "dist/",
    "src/",
    "config/",
    "data/",
    "README.md",
    "CLAUDE.md",
    "build-and-install.sh",
    ".env.example"
  ]
}
