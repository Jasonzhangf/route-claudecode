{
  "globalSettings": {
    "version": "1.0.0",
    "author": "Jason Zhang",
    "project": "claude-code-router",
    "createdAt": "2025-08-07T10:30:00Z"
  },
  "paths": {
    "inputLogs": "~/.route-claude-code/logs/",
    "outputData": "~/.route-claude-code/providers/",
    "tempDir": "/tmp/ccr-tools/",
    "configBackup": "~/.route-claude-code/config-backups/"
  },
  "performance": {
    "maxMemoryMB": 512,
    "timeoutSeconds": 30,
    "batchSize": 1000,
    "logLevel": "info"
  },
  "extraction": {
    "providers": [
      "anthropic",
      "codewhisperer", 
      "openai",
      "gemini"
    ],
    "categories": [
      "long-text",
      "normal-text", 
      "tool-calls"
    ],
    "timeFormat": "ISO8601",
    "includeMeta": true
  },
  "visualization": {
    "theme": "light",
    "colorScheme": "automatic",
    "maxColors": 20,
    "errorColor": "#FF0000",
    "successColor": "#00AA00",
    "timePrecision": "millisecond"
  },
  "dataRetention": {
    "maxDays": 30,
    "archiveAfterDays": 7,
    "cleanupEnabled": true
  }
}